{
  "name": "@bpmn-io/feel-editor",
  "version": "0.8.1",
  "description": "Editor for FEEL expressions.",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "scripts": {
    "all": "run-s lint test build",
    "compile:builtins": "node scripts/compileBuiltins.js",
    "test": "karma start",
    "build": "rollup -c",
    "build:watch": "rollup -cw",
    "builtins": "run-s pull:platform-docs compile:builtins",
    "lint": "eslint .",
    "pull:platform-docs": "git -C camunda-platform-docs pull || git clone git@github.com:camunda/camunda-platform-docs.git camunda-platform-docs",
    "start": "cross-env SINGLE_START=true npm run dev",
    "dev": "npm test -- --auto-watch --no-single-run",
    "prepare": "run-s build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bpmn-io/feel-editor.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">= 16"
  },
  "keywords": [
    "bpmn-io",
    "feel",
    "editor"
  ],
  "author": {
    "name": "Martin Stamm",
    "url": "https://github.com/marstamm"
  },
  "contributors": [
    {
      "name": "bpmn.io contributors",
      "url": "https://github.com/bpmn-io"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "@bpmn-io/feel-lint": "^0.2.0",
    "@codemirror/autocomplete": "^6.3.2",
    "@codemirror/commands": "^6.2.4",
    "@codemirror/language": "^6.3.1",
    "@codemirror/lint": "^6.4.0",
    "@codemirror/state": "^6.1.4",
    "@codemirror/view": "^6.5.1",
    "@lezer/highlight": "^1.1.6",
    "lang-feel": "^1.0.0",
    "min-dom": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.8",
    "@rollup/plugin-commonjs": "^23.0.2",
    "@rollup/plugin-json": "^5.0.1",
    "@testing-library/dom": "^8.17.1",
    "@testing-library/user-event": "^14.4.3",
    "babel-loader": "^9.1.3",
    "babel-plugin-istanbul": "^6.1.1",
    "chai": "^4.3.6",
    "cross-env": "^7.0.3",
    "eslint": "^8.44.0",
    "eslint-plugin-bpmn-io": "^1.0.0",
    "glob": "^8.0.3",
    "karma": "^6.3.19",
    "karma-chrome-launcher": "^3.1.1",
    "karma-coverage": "^2.2.0",
    "karma-debug-launcher": "^0.0.5",
    "karma-env-preprocessor": "^0.1.1",
    "karma-firefox-launcher": "^2.1.2",
    "karma-mocha": "^2.0.1",
    "karma-sinon-chai": "^2.0.2",
    "karma-webpack": "^5.0.0",
    "mocha": "^10.0.0",
    "mocha-test-container-support": "^0.2.0",
    "npm-run-all": "^4.1.5",
    "puppeteer": "^19.3.0",
    "rollup": "^3.3.0",
    "sinon": "^14.0.0",
    "sinon-chai": "^3.7.0",
    "webpack": "^5.72.1"
  }
}
